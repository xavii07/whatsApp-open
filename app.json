{
  "expo": {
    "name": "whatsLink",
    "slug": "whatsLink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "whatslink",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25D366"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#25D366"
      },
      "package": "com.xavii077.whatsLink"
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experimental": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b5e27654-948d-4888-a574-5ad2d93e08a5"
      }
    },
    "owner": "xavii077"
  }
}
